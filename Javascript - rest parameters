//rest parameters represent an indefinite number of parameters
//packs arguments into an array

let a = 1;
let b = 2;
let c = 9;
let d = 5;
let e = 6;
let f = 10;


console.log(sum(a, b, c, e, f));//The function belows allows us to specify as many arguments as we want into the function

function sum(...numbers){//syntax ... - followed by array name
    let total = 0
    for(let number of numbers){
        total += number
    }
    return total
}
