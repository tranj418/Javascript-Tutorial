// Const a variable that CANNOT be changed

// equation - C = 2(3.14)(r)

const PI = 3.14159; // will keep this value from being changed

let num1 = 2;
num1 = Number(num1)

let r;
r = window.prompt('Enter circle radius')
r = Number(r)

let circumference = num1 * PI * r

console.log('The circumference is', circumference)
